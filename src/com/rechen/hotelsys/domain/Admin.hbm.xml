<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.rechen.hotelsys.domain">

   <class name="Admin" table="tbl_admin">
   
      <id name="adminNo" column="admin_no" type="string" length="6">
         <generator class="assigned"></generator>
      </id>
      <property name="adminPwd" column="admin_pwd" not-null="true" type="string" length="6"></property>
      <property name="adminName" column="admin_name" not-null="true" type="string" length="30"></property>
     
   </class>

</hibernate-mapping>
