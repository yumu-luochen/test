<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.rechen.hotelsys.domain">

   <class name="FoodCategory" table="tbl_food_category" lazy="false">
   
      <id name="foodCategoryId" column="food_category_id" type="int">
         <generator class="increment"></generator>
      </id>
      <property name="foodCategoryName" column="food_category_name" not-null="true" type="string" length="20"></property>
		
   </class>

</hibernate-mapping>
